	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@tailwindcss/cli": "^4.1.18",
		"@tailwindcss/postcss": "^4.1.18",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.1",
		"@testing-library/user-event": "^14.6.1",
		"daisyui": "^5.5.14",
		"framer-motion": "^12.23.26",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-icons": "^5.5.0",
		"react-router-dom": "^7.11.0",
		"react-scripts": "^5.0.1",
